            "name": "Commit-0",
            "content": "@* Views/Ogrenci/Duzenle.cshtml *@\n@model OgrenciYonetimProjesi.Models.Ogrenci\n\n@{\n    ViewData[\"Title\"] = \"Öğrenciyi Düzenle\";\n}\n\n<h1>Öğrenciyi Düzenle</h1>\n\n<hr />\n<div class=\"row\">\n    <div class=\"col-md-4\">\n        <form asp-action=\"Duzenle\">\n            <div asp-validation-summary=\"ModelOnly\" class=\"text-danger\"></div>\n            \n            @* Güncellenecek kaydın ID'sini taşımak için gizli alan *@\n            <input type=\"hidden\" asp-for=\"Id\" />\n            \n            <div class=\"form-group mb-3\">\n                <label asp-for=\"Ad\" class=\"control-label\"></label>\n                <input asp-for=\"Ad\" class=\"form-control\" />\n                <span asp-validation-for=\"Ad\" class=\"text-danger\"></span>\n            </div>\n            \n            <div class=\"form-group mb-3\">\n                <label asp-for=\"Soyad\" class=\"control-label\"></label>\n                <input asp-for=\"Soyad\" class=\"form-control\" />\n                <span asp-validation-for=\"Soyad\" class=\"text-danger\"></span>\n            </div>\n            \n            <div class=\"form-group mb-3\">\n                <label asp-for=\"OgrenciNo\" class=\"control-label\"></label>\n                <input asp-for=\"OgrenciNo\" class=\"form-control\" />\n                <span asp-validation-for=\"OgrenciNo\" class=\"text-danger\"></span>\n            </div>\n            \n            <div class=\"form-group mt-3\">\n                <input type=\"submit\" value=\"Kaydet\" class=\"btn btn-primary\" />\n            </div>\n        </form>\n    </div>\n</div>\n\n<div class=\"mt-3\">\n    <a asp-action=\"Index\">Listeye Geri Dön</a>\n</div>\n\n@section Scripts {\n    @{await Html.RenderPartialAsync(\"_ValidationScriptsPartial\");}\n}"
